        <bpel:partnerLink name="client"
                     partnerLinkType="tns:WS_Integrator"
                     myRole="WS_IntegratorProvider"
                     />
        <bpel:partnerLink name="CreditCardPL" partnerLinkType="tns:CreditCardPLT" partnerRole="CreditCardPLRole"></bpel:partnerLink>
        <bpel:partnerLink name="SqliteLoggerServicePL" partnerLinkType="tns:SqliteLoggerServicePLT" partnerRole="SqliteLoggerServicePLR"></bpel:partnerLink>
    </bpel:partnerLinks>
  
    <!-- ================================================================= -->         
    <!-- VARIABLES                                                         -->
    <!-- List of messages and XML documents used within this BPEL process  -->
    <!-- ================================================================= -->         
    <bpel:variables>
        <!-- Reference to the message passed as input during initiation -->
        <bpel:variable name="input"
                  messageType="tns:WS_IntegratorRequestMessage"/>
                  
        <!-- 
          Reference to the message that will be returned to the requester
          -->
        <bpel:variable name="output"
                  messageType="tns:WS_IntegratorResponseMessage"/>
        <bpel:variable name="CreditCardPLResponse" messageType="ns:CheckCCSoapOut"></bpel:variable>
        <bpel:variable name="CreditCardPLRequest" messageType="ns:CheckCCSoapIn"></bpel:variable>
        <bpel:variable name="SqliteLoggerServicePLResponse" messageType="ns0:addLogResponse"></bpel:variable>
        <bpel:variable name="SqliteLoggerServicePLRequest" messageType="ns0:addLogRequest"></bpel:variable>
    </bpel:variables>

    <!-- ================================================================= -->         
    <!-- ORCHESTRATION LOGIC                                               -->
    <!-- Set of activities coordinating the flow of messages across the    -->
    <!-- services integrated within this business process                  -->
    <!-- ================================================================= -->         
    <bpel:sequence name="main">
        
        <!-- Receive input from requester. 
             Note: This maps to operation defined in WS_Integrator.wsdl 
             -->
        <bpel:receive name="receiveInput" partnerLink="client"
                 portType="tns:WS_Integrator"
                 operation="process" variable="input"
                 createInstance="yes"/>
        
        <!-- Generate reply to synchronous request -->
        <bpel:assign validate="no" name="AssignInputToCreditCardService">
            <bpel:copy>
                <bpel:from><bpel:literal><ns:addLog xmlns:ns="http://logger.axis" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <ns:cc_number>ns:cc_number</ns:cc_number>
  <ns:cc_type>ns:cc_type</ns:cc_type>
</ns:addLog>
</bpel:literal></bpel:from>
                <bpel:to variable="SqliteLoggerServicePLRequest" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from><bpel:literal><tns:CheckCC xmlns:tns="http://ws.cdyne.com/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:CardNumber>tns:CardNumber</tns:CardNumber>
</tns:CheckCC>
</bpel:literal></bpel:from>
                <bpel:to variable="CreditCardPLRequest" part="parameters"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:input]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="CreditCardPLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:CardNumber]]></bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:input]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="SqliteLoggerServicePLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns0:cc_number]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeCreditCardService" partnerLink="CreditCardPL" operation="CheckCC" portType="ns:LuhnCheckerSoap" inputVariable="CreditCardPLRequest" outputVariable="CreditCardPLResponse"></bpel:invoke>
        <bpel:assign validate="no" name="AssignOutputToResult">
            <bpel:copy>
                <bpel:from><bpel:literal><tns:WS_IntegratorResponse xmlns:tns="http://integrator.itec833" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>tns:result</tns:result>
</tns:WS_IntegratorResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="CreditCardPLResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:CheckCCResult/ns:CardType]]></bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="parameters" variable="CreditCardPLResponse">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:CheckCCResult/ns:CardType]]></bpel:query>
                </bpel:from>
                <bpel:to part="parameters" variable="SqliteLoggerServicePLRequest">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns0:cc_type]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="InvokeSqliteLoggerService" partnerLink="SqliteLoggerServicePL" operation="addLog" portType="ns0:SqliteLoggerServicePortType" inputVariable="SqliteLoggerServicePLRequest" outputVariable="SqliteLoggerServicePLResponse"></bpel:invoke>
        <bpel:reply name="replyOutput" 
               partnerLink="client"
               portType="tns:WS_Integrator"
               operation="process" 
               variable="output"
               />
    </bpel:sequence>
</bpel:process>

